module c.windows.d2derr;


import c.windows.windef;
import c.windows.winerror;
import c.windows.wincodec;


private enum FACILITY_D2D = 0x899;

private pure nothrow
HRESULT MAKE_D2DHR(WORD sev, WORD code)
{
	return MAKE_HRESULT(sev, FACILITY_D2D, code);
}

private pure nothrow
HRESULT MAKE_D2DHR_ERR(WORD code)
{
	return MAKE_D2DHR(1, code);
}

